<idea-plugin>
    <id>com.chase.dps.plugin.bluej-inspections</id>
    <name>DPS Inspections</name>
    <vendor email="simon.nektalov@chase.com" url="http://www.chase.com">JPMorgan Chase</vendor>

    <description>Custom DPS Inspections to suggest injection of annotations in a class
    </description>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <idea-version since-build="139.0" />
    <depends>com.intellij.modules.java</depends>
    <application-components>
        <component>
            <implementation-class>com.chase.dps.plugin.inspections.InspectionRegistration</implementation-class>
        </component>
    </application-components>
</idea-plugin>